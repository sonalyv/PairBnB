<%= simple_form_for @reservation do |f| %>
	<%= f.hidden_field :listing_id, value:"#{@listing.id}" %>
	<%= f.date_field :start_date, order: [:day, :month, :year], label: "Start Date" %>
	<%= f.date_field :end_date, order: [:day, :month, :year], label: "End Date"  %>
	<%= f.button :submit, :class => 'btn-custom' %>
<% end %> 


